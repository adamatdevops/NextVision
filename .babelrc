{
  "presets": [
		["airbnb"],
		[
			"@babel/env",
			{
				"targets": {
					"browsers": [
						"last 2 versions",
						"> 1%",
						"IE 11"
					]
				}
			}
		],
		[
			"@babel/preset-react",
			{
				"runtime": "automatic"
			}
		],
		["@babel/preset-typescript"],
		["babel-preset-vite"]
	]
	//"plugins": [
	//	"@babel/plugin-proposal-class-properties"
	//]
}
