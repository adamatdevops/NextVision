{
  "name": "tauri-client",
  "version": "0.3.101",
  "description": "Modern Tauri Desktop application that utilizes Rust, TypeScript & React",
  "type": "module",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@mantine/charts": "^7.6.1",
    "@mantine/code-highlight": "^7.6.1",
    "@mantine/core": "^7.6.1",
    "@mantine/dropzone": "^7.6.1",
    "@mantine/form": "^7.6.1",
    "@mantine/hooks": "^7.6.1",
    "@mantine/modals": "^7.6.1",
    "@mantine/notifications": "^7.6.1",
    "@mantine/nprogress": "^7.6.1",
    "@mantine/spotlight": "^7.6.1",
    "@mantine/styles": "^6.0.21",
    "@tauri-apps/api": "^1.5.3",
    "@vitejs/plugin-react": "4.2.1",
    "clsx": "^2.1.0",
    "material-icons": "^1.13.12",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.1",
    "recharts": "2",
    "vite": "5.1.4",
    "web-vitals": "^3.5.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "tsc && vite build",
    "test": "react-scripts test",
    "test:dev-package": "cross-env INSTALL_PACKAGE=1 yarn test",
    "test:dist": "cross-env NODE_ENV=production yarn test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-quick": "pretty-quick --staged && lint-staged",
    "eslint-dev": "ESLINT_USE_FLAT_CONFIG=false npx eslint --config .eslintrc.json",
    "dev": "vite",
    "clean": "tauri clean",
    "typecheck": "tsc --noEmit",
    "lint:typecheck": "tsc",
    "lint:eslint": "cross-env EFF_NO_LINK_RULES=true eslint . --format friendly",
    "lint:changelog": "node ./scripts/lint-changelog.js",
    "cache:prettier": "prettier . --check --cache",
    "check:prettier": "prettier --check 'src/**/*.tsx'",
    "preview": "vite preview",
    "format:prettier": "prettier-eslint . --write",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix --ignore-unknown",
    "fix:prettier": "yarn lint:prettier --write --ignore-unknown",
    "gen:changelog": "node ./scripts/generate-changelog.js",
    "debug": "node bin/prettier.js --ignore-path=.prettierignore"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@airbnb/config-jest": "^3.0.2",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^8.57.0",
    "@mantinex/mantine-logo": "^1.0.1",
    "@stylistic/eslint-plugin": "^1.6.2",
    "@stylistic/eslint-plugin-js": "^1.6.2",
    "@stylistic/eslint-plugin-jsx": "^1.6.2",
    "@stylistic/eslint-plugin-migrate": "^1.6.3",
    "@stylistic/eslint-plugin-plus": "^1.6.2",
    "@stylistic/eslint-plugin-ts": "^1.6.2",
    "@tauri-apps/cli": "^1.5.9",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/eslint": "^8.56.5",
    "@types/jest": "^29.5.12",
    "@types/js-beautify": "^1",
    "@types/node": "^20.11.20",
    "@types/prettier-linter-helpers": "^1",
    "@types/react": "18.2.56",
    "@types/react-dom": "18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@typescript-eslint/rule-tester": "^7.1.0",
    "@typescript-eslint/scope-manager": "^7.1.0",
    "@typescript-eslint/typescript-estree": "^7.1.0",
    "airbnb": "^0.0.2",
    "airbnb-browser-shims": "^3.3.0",
    "babel-preset-vite": "^1.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "esbuild": "^0.20.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-base-style-config": "^2.9.4",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "eslint-plugin-regex": "^1.10.0",
    "eslint-plugin-regexp": "^2.2.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-scope": "^8.0.0",
    "esm-utils": "^4.2.1",
    "get-tsconfig": "^4.7.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "js-beautify": "^1.15.1",
    "lint-staged": ">=10",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.1.0",
    "prettier-eslint": "^16.1.2",
    "prettier-eslint-cli": "^8.0.1",
    "prettier-linter-helpers": "^1.0.0",
    "prettier-plugin-properties": "^0.3.0",
    "prettier-plugin-rust": "^0.1.9",
    "pretty-quick": "^4.0.0",
    "react-ts-jest": "^1.0.0",
    "source-map-explorer": "^2.5.3",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "5.2.2",
    "typescript-eslint": "^7.1.0",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "resolutions": {
    "mini-css-extract-plugin": "2.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@4.1.0"
}
