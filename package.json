    },
    "peerDependencies": {
        "jiti": "1.21.0",
        "less": "*",
        "lightningcss": "^1.21.0",
        "meow": "^9.0.0",
        "optionator": "^0.9.1",
        "prettier-plugin-svelte": "^3.0.0",
        "sass": "*",
        "semantic-release": "^18.0.0",
        "stylus": "*",
        "svelte-eslint-parser": ">=0.30.0",
        "terser": "^5.4.0",
        "vite": "^5.0.0",
        "vls": "*",
        "vti": "*",
        "vue-tsc": ">=1.3.9"
