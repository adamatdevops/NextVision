{
    "name": "nextvision",
    "private": true,
    "version": "2.3.431",
    "description": "Modern React - Typescript - Vite Amplify Web application that utilizes AWS Amplify",
    "type": "module",
    "scripts": {
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "amplify:compile": "amplify api gql-compile",
        "amplify:push": "amplify push --yes",
        "amplify:push-destructive": "amplify push --yes --allow-destructive-graphql-schema-updates",
        "amplify:codegen": "amplify codegen && amplify api push",
        "amplify:rebuild": "amplify rebuild api",
        "babel:transpile-dev": "babel src/**/*.tsx js/dev",
        "babel:transpile-prod": "babel src/**/*.tsx js/prod",
        "build": "vite build",
        "build:tsc": "tsc && vite build",
        "cache:prettier": "prettier . --check --cache",
        "check:prettier": "prettier --check 'src/**/*.tsx'",
        "commit": "npx cz",
        "debug": "node bin/prettier.js --ignore-path=.prettierignore",
        "dev-clean": "npm run dev -- --force",
        "dev": "vite",
        "eject": "vite eject",
        "eslint-dev": "ESLINT_USE_FLAT_CONFIG=false npx eslint --config .eslintrc.json",
        "format:prettier": "prettier-eslint . --write",
        "fix": "run-s fix:eslint fix:prettier",
        "fix:eslint": "npm run lint:eslint --fix --ignore-unknown",
        "fix:prettier": "npm run lint:prettier --write --ignore-unknown",
        "gen:changelog": "node ./scripts/generate-changelog.js",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "lint:changelog": "node ./scripts/lint-changelog.js",
        "lint:eslint": "cross-env EFF_NO_LINK_RULES=true eslint . --format friendly",
        "lint:quick": "pretty-quick --staged && lint-staged",
        "lint:typecheck": "tsc",
        "prepare": "husky install",
        "preview": "vite preview",
        "serve": "vite preview --port 3000",
        "start": "vite start",
        "svgr": "svgr -d src/Icons/ assets/",
        "test:watch": "npm run test -- --watch",
        "test:all": "npm run test -- --silent --watchAll=false",
        "test:coverage": "npm run test:all -- --coverage",
        "test:dev-package": "cross-env INSTALL_PACKAGE=1 npm run test",
        "test:dist": "cross-env NODE_ENV=production npm run test",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@ant-design/icons": "^5.3.7",
        "@ant-design/icons-svg": "^4.4.2",
        "@ant-design/pro-layout": "^7.19.7",
        "@ant-design/pro-provider": "^2.14.7",
        "@ant-design/pro-utils": "^2.15.13",
        "@aws-amplify/ui-react": "^6.1.8",
        "@builder.io/dev-tools": "^1.0.12",
        "@builder.io/react": "^3.2.6",
        "@builder.io/sdk": "^2.2.2",
        "antd": "^5.17.3",
        "aws-amplify": "^6.0.28",
        "dayjs": "^1.11.11",
        "embla-carousel-react": "^8.0.4",
        "graphql": "^16.8.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-icons": "^5.0.1",
        "react-intl": "^6.6.8",
        "react-router-dom": "^6.22.1",
        "recharts": "^2.12.7",
        "web-vitals": "^3.5.2"
    },
    "devDependencies": {
        "@airbnb/config-jest": "^3.0.2",
        "@commitlint/cli": "^19.2.1",
        "@eslint/eslintrc": "^3.0.2",
        "@eslint/js": "^9.0.0",
        "@graphql-tools/schema": "^10.0.3",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@stylistic/eslint-plugin": "^1.7.0",
