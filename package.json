{
  "name": "nextvision",
  "private": true,
  "version": "2.1.0",
  "description": "Modern React - Typescript - Vite Amplify Web application that utilizes AWS Amplify",
  "type": "module",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "babel:transpile-dev": "babel src/**/*.tsx js/dev",
    "babel:transpile-prod": "babel src/**/*.tsx js/prod",
    "build": "vite build",
    "build:tsc": "tsc && vite build",
    "cache:prettier": "prettier . --check --cache",
    "check:prettier": "prettier --check 'src/**/*.tsx'",
    "commit": "npx cz",
    "debug": "node bin/prettier.js --ignore-path=.prettierignore",
    "dev-clean": "npm run dev -- --force",
    "dev": "vite",
    "eject": "vite eject",
    "eslint-dev": "ESLINT_USE_FLAT_CONFIG=false npx eslint --config .eslintrc.json",
    "format:prettier": "prettier-eslint . --write",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "npm lint:eslint --fix --ignore-unknown",
    "fix:prettier": "npm lint:prettier --write --ignore-unknown",
    "gen:changelog": "node ./scripts/generate-changelog.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:changelog": "node ./scripts/lint-changelog.js",
    "lint:eslint": "cross-env EFF_NO_LINK_RULES=true eslint . --format friendly",
    "lint:quick": "pretty-quick --staged && lint-staged",
    "lint:typecheck": "tsc",
    "prepare": "husky install",
    "preview": "vite preview",
    "serve": "vite preview --port 3000",
    "start": "vite start",
    "svgr": "svgr -d src/Icons/ assets/",
    "test:watch": "npm run test -- --watch",
    "test:all": "npm run test -- --silent --watchAll=false",
    "test:coverage": "npm run test:all -- --coverage",
    "test:dev-package": "cross-env INSTALL_PACKAGE=1 yarn test",
    "test:dist": "cross-env NODE_ENV=production yarn test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@tiptap/core": "^2.0.0",
    "@tiptap/extension-link": "^2.3.0",
    "@tiptap/react": "^2.3.0",
    "@tiptap/starter-kit": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "aws-amplify": "^6.0.28",
    "clsx": "^2.1.0",
    "csv": "^6.3.8",
    "dayjs": "^1.11.10",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "ra-data-json-server": "^4.16.15",
    "react": "^18.0.0",
    "react-admin": "^4.16.15",
    "react-admin-amplify": "^2.0.1",
    "react-dom": "^18.0.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.1",
    "recharts": "^2.12.5",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@airbnb/config-jest": "^3.0.2",
    "@commitlint/cli": "^19.2.1",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^1.7.0",
    "@stylistic/eslint-plugin-js": "^1.7.0",
    "@stylistic/eslint-plugin-jsx": "^1.7.0",
    "@stylistic/eslint-plugin-migrate": "^1.7.0",
    "@stylistic/eslint-plugin-plus": "^1.7.0",
    "@stylistic/eslint-plugin-ts": "^1.7.0",
    "@svgr/cli": "^8.1.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@tabler/icons-react": "^3.1.0",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.1",
    "@types/dateformat": "^5",
    "@types/eslint": "^8.56.9",
    "@types/jest": "^29.5.12",
    "@types/js-beautify": "^1.14.3",
    "@types/json-server": "^0.14.7",
    "@types/node": "^20.12.7",
    "@types/prettier-linter-helpers": "^1",
    "@types/react": "^18.2.66",
