{
  "name": "nvfrontend",
  "private": true,
  "version": "1.0.0",
	"description": "Modern AWS Amplify Web application that utilizes Rust, TypeScript & React",
  "type": "module",
  "scripts": {
		"analyze": "source-map-explorer 'build/static/js/*.js'",
		"babel:transpile-dev": "babel src/**/*.tsx js/dev",
    "babel:transpile-prod": "babel src/**/*.tsx js/prod",
		"build": "tsc && vite build",
		"cache:npm": "npm cache clean",
		"cache:prettier": "prettier . --check --cache",
		"check:prettier": "prettier --check 'src/**/*.tsx'",
		"commit": "npx cz",
		"debug": "node bin/prettier.js --ignore-path=.prettierignore",
		"dev-clean": "yarn run dev -- --force",
    "dev": "vite",
		"eject": "vite eject",
		"eslint-dev": "ESLINT_USE_FLAT_CONFIG=false npx eslint --config .eslintrc.json",
		"format:prettier": "prettier-eslint . --write",
		"fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "yarn lint:eslint --fix --ignore-unknown",
    "fix:prettier": "yarn lint:prettier --write --ignore-unknown",
		"gen:changelog": "node ./scripts/generate-changelog.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:changelog": "node ./scripts/lint-changelog.js",
		"lint:eslint": "cross-env EFF_NO_LINK_RULES=true eslint . --format friendly",
		"lint:quick": "pretty-quick --staged && lint-staged",
		"lint:typecheck": "tsc",
		"prepare": "husky install",
    "preview": "vite preview",
		"serve": "vite preview --port 3000",
		"start": "vite start",
		"svgr": "svgr -d src/Icons/ assets/",
		"test:watch": "yarn run test -- --watch",
    "test:all": "yarn run test -- --silent --watchAll=false",
    "test:coverage": "yarn run test:all -- --coverage",
    "test:dev-package": "cross-env INSTALL_PACKAGE=1 yarn test",
    "test:dist": "cross-env NODE_ENV=production yarn test",
		"typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ra-data-json-server": "^4.16.15",
    "react": "^18.2.0",
    "react-admin": "^4.16.15",
    "react-dom": "^18.2.0",
		"clsx": "^2.1.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.1",
    "recharts": "2",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
		"@airbnb/config-jest": "^3.0.2",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.24.1",
		"@commitlint/cli": "^19.2.1",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^8.57.0",
		"@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^13.0.0",
		"@stylistic/eslint-plugin": "^1.6.2",
    "@stylistic/eslint-plugin-js": "^1.6.2",
    "@stylistic/eslint-plugin-jsx": "^1.6.2",
    "@stylistic/eslint-plugin-migrate": "^1.6.3",
    "@stylistic/eslint-plugin-plus": "^1.6.2",
    "@stylistic/eslint-plugin-ts": "^1.6.2",
		"@svgr/babel-plugin-svg-em-dimensions": "^8.0.0",
    "@svgr/babel-plugin-transform-svg-component": "^8.0.0",
    "@svgr/babel-preset": "^8.1.0",
		"@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
		"@tabler/icons-react": "^3.0.0",
		"@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
		"@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/eslint": "^8.56.5",
    "@types/jest": "^29.5.12",
    "@types/js-beautify": "^1",
    "@types/prettier-linter-helpers": "^1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
		"@typescript-eslint/rule-tester": "^7.1.0",
    "@typescript-eslint/scope-manager": "^7.1.0",
    "@typescript-eslint/typescript-estree": "^7.1.0",
    "@vitejs/plugin-react": "^4.2.1",
		"@vitejs/plugin-react-refresh": "^1.3.6",
		"airbnb": "^0.0.2",
    "airbnb-browser-shims": "^3.3.0",
    "astroturf": "^1.2.0",
    "autoprefixer": "^10.4.18",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "babel-preset-vite": "^1.1.3",
    "badge-maker": "^3.3.1",
    "browserslist": "^4.23.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "cssnano": "^6.1.2",
    "cssnano-preset-advanced": "^6.1.2",
    "cssnano-preset-default": "^6.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "dotenv": "^16.4.5",
    "emoji-regex": "^10.3.0",
    "esbuild": "^0.20.1",
    "eslint": "^8.57.0",
		"eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
		"eslint-plugin-base-style-config": "^2.9.4",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
		"eslint-plugin-regex": "^1.10.0",
    "eslint-plugin-regexp": "^2.2.0",
    "eslint-plugin-unused-imports": "^3.1.0",
		"eslint-scope": "^8.0.0",
    "esm-utils": "^4.2.1",
    "get-tsconfig": "^4.7.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "js-beautify": "^1.15.1",
    "lint-staged": ">=10",
		"postcss": "^8.4.35",
		"postcss-cli": "^11.0.0",
    "postcss-load-config": "^5.0.3",
    "postcss-mixins": "^10.0.0",
    "postcss-plugin": "^1.0.0",
    "postcss-preset-env": "^9.5.2",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "postcss-svgo": "^6.0.3",
		"precss": "^4.0.0",
    "prettier": "^3.1.0",
    "prettier-eslint": "^16.1.2",
    "prettier-eslint-cli": "^8.0.1",
    "prettier-linter-helpers": "^1.0.0",
    "prettier-plugin-properties": "^0.3.0",
    "prettier-plugin-rust": "^0.1.9",
    "pretty-quick": "^4.0.0",
    "react-ts-jest": "^1.0.0",
    "source-map-explorer": "^2.5.3",
    "sugarss": "^4.0.1",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
		"vite-plugin-checker": "^0.6.4",
    "vite-plugin-env-compatible": "^2.0.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
	"config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "commit",
    "pretty",
    "format",
    "conventional changelog",
    "commitizen"
  ],
  "resolutions": {
    "mini-css-extract-plugin": "2.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss}": [
      "prettier --write"
    ]
  }
}
