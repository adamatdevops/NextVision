{
	"printWidth": 120,
	"singleQuote": false,
	"htmlWhitespaceSensitivity": "strict",
	"trailingComma": "all",
	"useTabs": true,
	"tabWidth": 2,
	"semi": true,
	"quoteProps": "preserve",
	"jsxSingleQuote": false,
	"bracketSpacing": false,
	"bracketLine": true,
	"parser": "typescript",
	"overrides": [
		{
			"files": ["*.html"],
			"options": {
				"parser": "html"
			}
		},
		{
			"files": ["*.css"],
			"options": {
				"parser": "css"
			}
		},
		{
			"files": "*.properties",
			"options": {
				"printWidth": 0
			}
		},
		{
			"files": ["**/*.rs"],
			"options": {
				"printWidth": 80
			}
		}
	]
}
